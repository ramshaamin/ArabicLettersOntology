<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.tajweedontology.org/ontologies/rules#"
     xml:base="http://www.tajweedontology.org/ontologies/rules"
     xmlns:ns="http://creativecommons.org/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:rules="http://www.tajweedontology.org/ontologies/rules#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:partof="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#">
    <owl:Ontology rdf:about="http://www.tajweedontology.org/ontologies/rules">
        <terms:creator>Ramsha Amin</terms:creator>
        <terms:description>An ontology about Arabic Letters and their characteristics in the recitation of The Holy Quran. The ontology is annotated in both Arabic and English.</terms:description>
        <terms:license>https://creativecommons.org/licenses/by/2.0/</terms:license>
        <rdfs:label xml:lang="en">Tajweed Rules</rdfs:label>
        <rdfs:label xml:lang="ar">قواعد التجويد</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/created -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/2002/07/owl#sameAs -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#AreaOfArticulation"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf">
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#AreaOfArticulation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#belongToGroup -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#belongToGroup">
        <owl:inverseOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#belongToLetter"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#GroupOfLetters"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#belongToLetter -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#belongToLetter">
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#GroupOfLetters"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#composeWord -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#composeWord">
        <owl:inverseOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#containLetter"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#Word"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#containLetter -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#containLetter">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#Word"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasArticulationPoint -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasArticulationPoint">
        <owl:inverseOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#isArticulationPointof"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasCharacteristic -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasCharacteristic">
        <owl:inverseOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#isCharacteristicOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#BasicCharacteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasCharacteristicType -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasCharacteristicType">
        <owl:inverseOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#isCharacteristicTypeOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#BasicCharacteristic"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#TypesOfCharacteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasOpposite -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasOpposite">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithOpposite"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithOpposite"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasPartDirectly -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasPartDirectly">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart"/>
        <owl:inverseOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#isPartOfDirectly"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#involveArticulationArea -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#involveArticulationArea">
        <owl:inverseOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#involveLetter"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#AreaOfArticulation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#involveArticulationPoint -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#involveArticulationPoint">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#involveLetter -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#involveLetter">
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#AreaOfArticulation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#involvePartially -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#involvePartially">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithOpposite"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithOpposite"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#isArticulationPointof -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#isArticulationPointof">
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#isCharacteristicOf -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#isCharacteristicOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#BasicCharacteristic"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#isCharacteristicTypeOf -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#isCharacteristicTypeOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.tajweedontology.org/ontologies/rules#TypesOfCharacteristic"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#BasicCharacteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#isPartOfDirectly -->

    <owl:ObjectProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#isPartOfDirectly">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasAppliedTajweedDefinition -->

    <owl:DatatypeProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasAppliedTajweedDefinition">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Definitions of the concepts or instances that are applied in practical learning of Tajweed</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasExplaination -->

    <owl:DatatypeProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasExplaination">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasLinguisticDefinition -->

    <owl:DatatypeProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasLinguisticDefinition">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Definition that is relating to language or lingustics</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#hasSheikhAlJazareeReference -->

    <owl:DatatypeProperty rdf:about="http://www.tajweedontology.org/ontologies/rules#hasSheikhAlJazareeReference">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>This is the matn (poem) of Muhammad bin Muhammad Bin Muhammad Bin &apos;Alee Bin Yuusuf Ibn Al Jazaree, who died in 833 Hijri, &quot;Al Muqaddimatu al Jazariyyah&quot; who has 109 verses, speaking of the rules of tajweed (art of Quranic recitation) and makhaarij Al huruuf (knowing the places from which the Arabic letters are pronounced). The poem is orignally in Arabic.

In our ontology we have annotated it in English as well.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.tajweedontology.org/ontologies/rules#ApproximateArticulationPoint -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#ApproximateArticulationPoint">
        <rdfs:subClassOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment xml:lang="en">It is that which does not rely on a specific place from the area of the throat, or the tongue, or the two lips. This approximate articulation point is applied to the lengthened letters that comes from the empty spaces in the mouth and throat.</rdfs:comment>
        <rdfs:comment xml:lang="ur">وہ مخرج ہے جس کا انحصار ،حلق ،زبان یا ہونٹوں کے کسی خاص حصہ پر نہیں ہوتا۔اس کا اطلاق منہ اور حلق کے خالی حصہ  اور خیشوم سے ہوتا ہے۔</rdfs:comment>
        <rdfs:label xml:lang="ar">المخرج المقدر</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#AreaOfArticulation -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#AreaOfArticulation">
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#Characteristic"/>
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#Item"/>
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
        <rdfs:label xml:lang="en">AreaOfArticulation</rdfs:label>
        <rdfs:label xml:lang="ar">اصول_مخارج</rdfs:label>
        <rdfs:label xml:lang="ur">مخارج کى حصہ</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPoint -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf"/>
                <owl:someValuesFrom rdf:resource="http://www.tajweedontology.org/ontologies/rules#AreaOfArticulation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tajweedontology.org/ontologies/rules#isArticulationPointof"/>
                <owl:someValuesFrom rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#Characteristic"/>
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#Item"/>
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
        <rdfs:label xml:lang="en">ArticulationPoint</rdfs:label>
        <rdfs:label xml:lang="ar">مخارج الحروف</rdfs:label>
        <rdfs:label xml:lang="ur">مخارج کى جگہ</rdfs:label>
        <owl:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/page/Place_of_articulation</owl:sameAs>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#BasicCharacteristic -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#BasicCharacteristic">
        <rdfs:subClassOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#Characteristic"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tajweedontology.org/ontologies/rules#hasCharacteristicType"/>
                <owl:someValuesFrom rdf:resource="http://www.tajweedontology.org/ontologies/rules#TypesOfCharacteristic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#ConditionalCharacteristic"/>
        <rdfs:comment xml:lang="en">There are eight characteristics in pair;each characteristic of the pair is opposite to the other characteristic in the pair. There are also seven singular characteristics that have no opposites. Each letter has atleast four characteristics, one of each of the pair of opposite.</rdfs:comment>
        <rdfs:label xml:lang="ar">الصفات الاصلية</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#Characteristic -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#Characteristic">
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#Letter"/>
        <rdfs:isDefinedBy xml:lang="en">What serves the purpose of describing the meaning, This can be in thw way of physical descriptions such as white and black, or can be abstract descriptions such as education</rdfs:isDefinedBy>
        <rdfs:isDefinedBy xml:lang="ur">اصطلاحی معنیٰ: وہ حالت یا کیفیت جو مخرج سے ادا ہوتے وقت حرف میں پائی جاتی ہے۔ جو اس حرف کو دوسرے حروف سے ممتاز /الگ کرتی ہے۔</rdfs:isDefinedBy>
        <rdfs:isDefinedBy xml:lang="ur">لفظی معنیٰ:  کیفیت یا حالت</rdfs:isDefinedBy>
        <rdfs:isDefinedBy xml:lang="ar">هي الكيفية العارضة للحرف عند حصوله فى المخرج والي تمي</rdfs:isDefinedBy>
        <rdfs:isDefinedBy xml:lang="ar">هي ما قام بالشي ء من المعاني</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Characteristic</rdfs:label>
        <rdfs:label xml:lang="ur">صفات</rdfs:label>
        <rdfs:label xml:lang="ar">صفات الحرف</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithOpposite -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithOpposite">
        <rdfs:subClassOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#BasicCharacteristic"/>
        <owl:disjointWith rdf:resource="http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithoutOpposite"/>
        <rdfs:label xml:lang="ar">صفات ذوات الاضدا</rdfs:label>
        <rdfs:label xml:lang="ur">صفات متضادہ</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithoutOpposite -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#CharacteristicsWithoutOpposite">
        <rdfs:subClassOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#BasicCharacteristic"/>
        <rdfs:label xml:lang="ur">صفات غیرمتضادہ</rdfs:label>
        <rdfs:label xml:lang="ar">صفات لا اضداد لها</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ConditionalCharacteristic -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#ConditionalCharacteristic">
        <rdfs:subClassOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#Characteristic"/>
        <rdfs:label xml:lang="ar">الصفات العرضية</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#GroupOfLetters -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#GroupOfLetters"/>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#HaroofAshshafawiya -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#HaroofAshshafawiya">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.tajweedontology.org/ontologies/rules#Letter"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.tajweedontology.org/ontologies/rules#hasArticulationPoint"/>
                        <owl:hasValue rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointofTwoLips"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.tajweedontology.org/ontologies/rules#Letter"/>
                            <owl:Restriction>
