    <!-- http://www.tajweedontology.org/ontologies/rules#Letter -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#Letter">
        <rdfs:subClassOf rdf:resource="http://www.tajweedontology.org/ontologies/rules#Item"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.tajweedontology.org/ontologies/rules#hasArticulationPoint"/>
                        <owl:someValuesFrom rdf:resource="http://www.tajweedontology.org/ontologies/rules#AreaOfArticulation"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.tajweedontology.org/ontologies/rules#hasArticulationPoint"/>
                        <owl:someValuesFrom rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tajweedontology.org/ontologies/rules#hasCharacteristic"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.tajweedontology.org/ontologies/rules#Characteristic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Letter</rdfs:label>
        <rdfs:label xml:lang="ar">الحروف</rdfs:label>
        <rdfs:label xml:lang="ur">حرف</rdfs:label>
        <owl:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/page/Arabic_alphabet</owl:sameAs>
    </owl:Class>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#MaadLetter -->

    <owl:Class rdf:about="http://www.tajweedontology.org/ontologies/rules#MaadLetter">
