    <!-- http://www.tajweedontology.org/ontologies/rules#ArticuationPointofو -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticuationPointofو">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#CircleofTheTwoLips"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticuationPointofو"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>The unlengthened wow is articulated by forming a circle of the two lips without thetwo lips meeting completely.</rdfs:comment>
        <rdfs:label xml:lang="ur">دونوں ہونٹوں کے ملنے سے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointE1 -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointE1">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationArea rdf:resource="http://www.tajweedontology.org/ontologies/rules#EmptySpacesinMouthandThroat"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointE1"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the articulation point for the Maad letters.</rdfs:comment>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointT1 -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointT1">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#DeepestPartofThroat"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointT1"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the articulation point for the letters ء ه. It is articulated from the deepest part of the throat.</rdfs:comment>
        <rdfs:label xml:lang="ur">حلق کا سب سے گہرے حصے سے ادا ہوتے ہے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointT2 -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointT2">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#MiddlePartofThroat"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointT2"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the articulation point for the letters ح, ع. It is articulated from the middle part of the throat.</rdfs:comment>
        <rdfs:label xml:lang="ur">حلق کا درمیانی حصے سے ادا ہوتے ہے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointT3 -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointT3">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#ClosestPartofThroat"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointT3"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the articulation point for the letters خ,غ. It is articulated from the closest part of the throat.</rdfs:comment>
        <rdfs:label xml:lang="ur">حلق کا قریب ترین حصے سے ادا ہوتے ہے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointTipofTongue -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointTipofTongue">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#BetweenTwoTopIncisors"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#SlightlyAboveLowerIncisors"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TipofTongue"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointTipofTongue"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letters ز, س, ص. They are articulated between Tip of the tongue and between the plates of two top incisors and slightly above lower incisors.</rdfs:comment>
        <rdfs:label xml:lang="ur">طرف اللسان جب ثنایا سفلیٰ کےاندرونی کناروں سے لگے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointTipofTongueandBottomEdges -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointTipofTongueandBottomEdges">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#BottomEdgesofTwoUpperIncisors"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TipofTongue"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointTipofTongueandBottomEdges"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letters ث, ذ, ظ. These three letters are articulated from between the Tip of the tongue, from the top side of the tip and bottom edges of the two top incisors.</rdfs:comment>
        <rdfs:label xml:lang="ur">طرف اللسان جب ثنایا علیا کے کناروں سے لگے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointofMiddlePartofTongue -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointofMiddlePartofTongue">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#MiddlePartofTongue"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#UpperPalate"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointofMiddlePartofTongue"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the articulation point for the letters ي ش. It is articulated from the middle part of the tongue and opposite top palate
.</rdfs:comment>
        <rdfs:label xml:lang="ur">وسط اللسان بالمقابل اوپرکاتالو سے لگے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointofTopoftheTongue -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointofTopoftheTongue">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#GumLineofUpperTwoIncisors"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TopofTipofTongue"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointofTopoftheTongue"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for letter ط, ت,د . They are articulated from Gum line of front two top incisors and Top of the tongue.</rdfs:comment>
        <rdfs:label xml:lang="ur">طرف اللسان جب  ثنایا علیا کی جڑسےلگے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointofTwoLips -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointofTwoLips">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#ClosingTwoLips"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointofTwoLips"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letters. letter involves articulation area nose for its complete pronounciation</rdfs:comment>
        <rdfs:label xml:lang="ur">دونوں ہونٹوں کو ملانے سے ادا ہوتا ہے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointر -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointر">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#OppositetotheGumofTwoTopFrontIncisors"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TopofTipofTongue"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointر"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letter ر. This letter is articulated from the tip of the tongue with top of the tip, close to the articulation point of ن and what lies opposite to it of the gums of the two top front incisors.</rdfs:comment>
        <rdfs:label xml:lang="ur">طرف اللسان  مع ظھراللسان جب ثنایا علیا کے مسوڑھوں سے لگے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointض -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointض">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#EdgeofTongue"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#UpperMolarsLeftorRightorboth"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointض"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letter ض. They are articulated from one of the sides or edges of the tongue and what lies opposite to it of the upper molars left or right. It can also be articulated from both sides at the same time.</rdfs:comment>
        <rdfs:label xml:lang="ur">جب حافۃ اللسان  بالمقابل اضراس علیا (ضواحک،طواحن،نواجذ) سے لگے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointف -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointف">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#InsideLowerLip"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TipofTwoFrontIncisors"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointف"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letter ف. They are articulated between the inside of the lower lip and the tips of the two front incisors.</rdfs:comment>
        <rdfs:label xml:lang="ur">جب ثنایا علیا کے کنارے نچلے ہونٹ کے اندرونی حصہ سے لگیں</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointق -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointق">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#DeepestPartofTongue"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#SoftPalate"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointق"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letter ق. They are articulated from the deepest part of the tongue and what lies opposite to it of the roof of the mouth (the soft palate)</rdfs:comment>
        <rdfs:label xml:lang="ur">اقصی اللسان بالمقابل نرم تالو</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointك -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointك">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#DeepestPartofTongue"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#HardPalate"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointك"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letter ك. They are articulated from the deepest part of the tongue and what lies opposite to it of the roof of the mouth (the hard palate). The ك is under the ق a little, meaning closer to the mouth and farther from the throat than the ق</rdfs:comment>
        <rdfs:label xml:lang="ur">اقصی اللسان بالمقابل نرم اور سخت تالو</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointل -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointل">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#EdgeofTongue"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#OppositetotheGumofFirstTwoUpperPreMolars"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TwoUpperCanines"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TwoUpperFrontIncisors"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TwoUpperLateralIncisors"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointل"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letter ل. They are articulated from the nearest part of the sides of the tongue and the end of its tip and what lies opposite to it of the gums of the first two upper premolars, the two upper canines, the two upper lateral incisors and the two upper front incisors.</rdfs:comment>
        <rdfs:label xml:lang="ur">طرفاللسان مع ادنیٰ حافہ جب  اسنان علیا(ثنایا،رباعی،انیاب )اورضواحک کے مسوڑھوں سے لگے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ArticulationPointن -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ArticulationPointن">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#OppositetotheGumofTwoTopFrontIncisors"/>
        <involveArticulationPoint rdf:resource="http://www.tajweedontology.org/ontologies/rules#TopofTipofTongue"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPointن"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:comment>This is the composite articulation point for the letter ن. They are articulated from the tip of the tongue and what lies opposite to it of the gums of the two top front incisors, slightly beneath the articulation point for the ل.</rdfs:comment>
        <rdfs:label xml:lang="ur">طرف اللسان جب ثنایا علیا کے مسوڑھوں سے لگے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#BetweenTwoTopIncisors -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#BetweenTwoTopIncisors">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <isPartOfDirectly rdf:resource="http://www.tajweedontology.org/ontologies/rules#Teeth"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#BetweenTwoTopIncisors"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:label xml:lang="en">Between Two Top Incisors</rdfs:label>
        <rdfs:label xml:lang="ur">سامنے کے دو دانتوں  کے درمیان</rdfs:label>
        <rdfs:label xml:lang="ar">من صغحتى الثنايا العليا</rdfs:label>
        <owl:sameAs>http://dbpedia.org/page/Incisor</owl:sameAs>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#BottomEdgesofTwoUpperIncisors -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#BottomEdgesofTwoUpperIncisors">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <isPartOfDirectly rdf:resource="http://www.tajweedontology.org/ontologies/rules#Teeth"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#BottomEdgesofTwoUpperIncisors"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:label xml:lang="en">Bottom Edges of Two Top Incisors</rdfs:label>
        <rdfs:label xml:lang="ar">أطراف الثنايا العليا أى رؤوسها</rdfs:label>
        <rdfs:label xml:lang="ur">سامنے کے دو دانتوں کا نیچے والا کنارہ</rdfs:label>
        <owl:sameAs>http://dbpedia.org/page/Incisor</owl:sameAs>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#CircleofTheTwoLips -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#CircleofTheTwoLips">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#CircleofTheTwoLips"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:label xml:lang="ur">دونوں ہونٹوں کے گولائی  سے</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ClosestPartofThroat -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ClosestPartofThroat">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <isPartOfDirectly rdf:resource="http://www.tajweedontology.org/ontologies/rules#Throat"/>
        <hasExplaination rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It means closest to the mouth. From here two letters emerge.</hasExplaination>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ClosestPartofThroat"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:label xml:lang="en">Closest Part of Throat</rdfs:label>
        <rdfs:label xml:lang="ar">ادنى الحلق</rdfs:label>
        <rdfs:label xml:lang="ur">حلق کا قریب ترین حصہ</rdfs:label>
        <owl:sameAs>http://dbpedia.org/page/Throat</owl:sameAs>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ClosestPartofThroat"/>
        <owl:annotatedProperty rdf:resource="http://www.tajweedontology.org/ontologies/rules#hasExplaination"/>
        <owl:annotatedTarget rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It means closest to the mouth. From here two letters emerge.</owl:annotatedTarget>
        <rdfs:label xml:lang="ur">اس کا مطلب منہ کے قریب ہے۔ یہاں سے دو حروف نکلتے ہیں۔</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#ClosingTwoLips -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#ClosingTwoLips">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <isPartOfDirectly rdf:resource="http://www.tajweedontology.org/ontologies/rules#TwoLips"/>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#ClosingTwoLips"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:label xml:lang="en">Closing Two Lips</rdfs:label>
        <rdfs:label xml:lang="ar">بين الشفتين معا بانطباقهما</rdfs:label>
        <rdfs:label xml:lang="ur">دونوں ہونٹوں کے ملنے سے</rdfs:label>
        <owl:sameAs>http://dbpedia.org/page/Lip</owl:sameAs>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#DeepestPartofThroat -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#DeepestPartofThroat">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <isPartOfDirectly rdf:resource="http://www.tajweedontology.org/ontologies/rules#Throat"/>
        <hasExplaination rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It is farthest from the mouth and closest to the chest. From here two letters are articulated.</hasExplaination>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#DeepestPartofThroat"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:label xml:lang="en">Deepest Part of Throat</rdfs:label>
        <rdfs:label xml:lang="ar">اقصى الحلق</rdfs:label>
        <rdfs:label xml:lang="ur">حلق کا سب سے گہرا حصہ</rdfs:label>
        <owl:sameAs>http://dbpedia.org/page/Throat</owl:sameAs>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#DeepestPartofThroat"/>
        <owl:annotatedProperty rdf:resource="http://www.tajweedontology.org/ontologies/rules#hasExplaination"/>
        <owl:annotatedTarget rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It is farthest from the mouth and closest to the chest. From here two letters are articulated.</owl:annotatedTarget>
        <rdfs:label xml:lang="ur">یہ منہ سے سب سے دور اور سینے کے قریب ہے۔ یہاں سے دو حروف نکلتے ہیں۔</rdfs:label>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#DeepestPartofTongue -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#DeepestPartofTongue">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <isPartOfDirectly rdf:resource="http://www.tajweedontology.org/ontologies/rules#Tongue"/>
        <hasSheikhAlJazareeReference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">أدناه غينٌ خاؤها والقافُ  أقصى اللسانِ فوق ثم الكافُ أسفل</hasSheikhAlJazareeReference>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#DeepestPartofTongue"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:label xml:lang="en">Deepest Part of Tongue</rdfs:label>
        <rdfs:label xml:lang="ar">اقصى اللسان</rdfs:label>
        <rdfs:label xml:lang="ur">زبان کی جڑ  یا آخری حصہ</rdfs:label>
        <owl:sameAs>http://dbpedia.org/page/Tongue</owl:sameAs>
    </owl:Axiom>
    


    <!-- http://www.tajweedontology.org/ontologies/rules#EdgeofTongue -->

    <owl:NamedIndividual rdf:about="http://www.tajweedontology.org/ontologies/rules#EdgeofTongue">
        <rdf:type rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <isPartOfDirectly rdf:resource="http://www.tajweedontology.org/ontologies/rules#Tongue"/>
        <hasSheikhAlJazareeReference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">والضاد من حافته إذ وَلِيَا  لاضراسَ من أيسرَ أو يمناها واللام أدناها لمنتهاها</hasSheikhAlJazareeReference>
    </owl:NamedIndividual>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.tajweedontology.org/ontologies/rules#EdgeofTongue"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
        <owl:annotatedTarget rdf:resource="http://www.tajweedontology.org/ontologies/rules#ArticulationPoint"/>
        <rdfs:label xml:lang="en">Edge of Tongue</rdfs:label>
        <rdfs:label xml:lang="ar">حافۃ اللسان</rdfs:label>
